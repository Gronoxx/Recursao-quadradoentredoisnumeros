#include <stdio.h>
int iq (int a, int b)
{
    if (a>b)
    return iq (b,a);
    if((a==b)||(a==(b-1)))
    return 0;
    if (a<(b-1))
    {
        printf("%d\n", (a+1)*(a+1));
        return iq (a+1,b);
    }
}
int main()
{
    int a,b;
    printf("Insira dois numeros para calcular todos os quadrados entre eles:");
    scanf("%i",&a);
    scanf("%i",&b);
    iq(a,b);

    return 0;
}
